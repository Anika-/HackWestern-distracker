<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <!--Style Trial-->

    <style name="Theme.Meuestilo" parent="Theme.AppCompat.Light.DarkActionBar" />

    <style name="ActionBar.Solid.Meuestilo" parent="@style/Widget.AppCompat.ActionBar.Solid">

    </style>

    <style name="Theme.Meuestilo.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Meuestilo.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Meuestilo.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>
